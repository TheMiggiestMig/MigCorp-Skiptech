<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <HediffDef>
    <defName>MigcorpSkipShock</defName>
    <label>skip-shock</label>
    <defaultLabelColor>(0.9, 1.0, 0.35)</defaultLabelColor>
    <hediffClass>HediffWithComps</hediffClass>
    <isBad>false</isBad>
    <!--tendable>false</tendable-->
    <initialSeverity>1</initialSeverity>
    <description>Your insides are still "inside". But as a result of an unstable skip, for the moment... they're not quite where they should be.</description>
    <stages>
      <li>
        <vomitMtbDays>0.02</vomitMtbDays>
        <painOffset>0.2</painOffset>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.7</setMax>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.15</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.3</offset>
          </li>
        </capMods>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_Disappears">
        <disappearsAfterTicks>600</disappearsAfterTicks>
        <showRemainingTime>true</showRemainingTime>
      </li>
    </comps>
  </HediffDef>
</Defs>